{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "strict": false,
    "sourceMap": true,
    "noImplicitAny": false,
    "skipLibCheck": true,
    "allowJs": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"]
}
